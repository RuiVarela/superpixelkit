  "bugs": {
    "url": "https://github.com/RuiVarela/superpixelkit/issues"
  },
  "homepage": "https://github.com/RuiVarela/superpixelkit/blob/master/README.md",
  "dependencies": {
    "isomorphic-ws": "^4.0.1",
    "serialport": "^6.2.2",
    "uuid": "^3.3.2",
    "ws": "^6.0.0"
  }
